        <h3 class="clearfix">
          <span class="text-title">
            this is a title
          </span>
          <nav class="block-actions">
            <ul>
              <li>
                <%= link_to raw('<i class="ion ion-arrow-up-b"></i>'), "#!" %>
              </li>
              <li>
                <%= link_to raw('<i class="ion ion-trash-a"></i>'), "#!" %>
              </li>
            </ul>
          </nav>
        </h3>
        <div class="grid-stack-item-body">
        <i class=""></i>
        </div>
        <%# render 'admin/dashboard/charts/chart_pie_partners_quantity' %>
